<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <vType id="default" maxspeed="25" accel="1.5" decel="3.0" emergencyDecel="4.0" 
       impatience="0.2" lcStrategic="0.5" lcCooperative="5.0" 
       jmSigmaMinor="1" jmIgnoreFoeSpeed="10" jmTimegapMinor="1.0" 
       jmIgnoreJunctionFoeProb="1" jmIgnoreKeepClearTime="1" 
       carFollowModel="EIDM" minGap="2.5" tau="1.5" 
       sigma="0.2" collisionMinGapFactor="0.3" sigmagap="0.4" sigmaerror="0.3" 
       color="0.5,0.5,0.5"/>
    <vType id="ego_vehicle" maxSpeed="60" accel="11.0" decel="13.0" emergencyDecel="14.0" 
       minGap="1.0" tau="0.4" collisionMinGapFactor="0.3" impatience="1.0" 
       laneChangeModel="LC2013" lcAssertive="50" lcStrategic="5.0" lcCooperative="0.1" 
       lcSpeedgain="10.0" lcPushy="10" lcImpatience="10" jmIgnoreFoeProb="1.0" 
       carFollowModel="EIDM" sigmagap="0.9" sigmaerror="0.9"
       color="1.0,0.0,0.0">
        <param key="has.ssm.device" value="true"/>
        <param key="device.ssm.measures" value="TTC DRAC PET MDRAC BR SGAP TGAP"/>
        <param key="device.ssm.thresholds" value="3.0 3.0 2.0 3.4 1.0 2.0 3.0"/>
        <param key="device.ssm.range" value="50.0" />
        <param key="device.ssm.mdrac.prt" value="1.0" />
        <param key="device.ssm.extratime" value="5.0" />
        <param key="device.ssm.file" value="ssm_v0.xml" />
        <param key="device.ssm.trajectories" value="false" />
        <param key="device.ssm.geo" value="false" />
        <param key="device.ssm.write-positions" value="false" />
        <param key="device.ssm.write-lane-positions" value="false" />
        <param key="device.ssm.filter-edges.input-file" value="input_list.txt" />
        <param key="device.ssm.exclude-conflict-types" value="" />
    </vType>    
    
    <route id="ego_route" edges= "-E29 E27 E28 E31"/>

    
    <!-- Flows for Default vehicles -->
    <flow id="flow_default_N" type="default" begin="0" end="200" number="10" from="-E30" to="E31" departLane="free" departSpeed="max"/>
    <flow id="flow_default_E" type="default" begin="5" end="200" number="10" from="-E31" to="E29" departLane="free" departSpeed="max"/>
    <flow id="flow_default_S" type="default" begin="10" end="200" number="10" from="-E32" to="E30" departLane="free" departSpeed="max"/>
    <flow id="flow_default_W" type="default" begin="15" end="200" number="10" from="-E29" to="E31" departLane="free" departSpeed="max"/>
    <vehicle id="ego_car" type="ego_vehicle" route="ego_route" depart="50" departLane="free" departSpeed="max"/>

</routes>
