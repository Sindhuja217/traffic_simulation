<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <!-- VTypes -->
    <vTypeDistribution id="vd1">
        <vType id="passive" guiShape="passenger" carFollowModel="EIDM" color="red" probability="0.25" tau="1.75" minGap="2.5" accel="1" decel="1" maxSpeed="10" />
        <vType id="agressive" guiShape="passenger" carFollowModel="EIDM" color="red" probability="0.25" tau="0.25" minGap="1" accel="5" decel="5" sigmaerror="0.5" maxSpeed="60" emergencyDecel="5" />
        <vType id="voilator" guiShape="passenger" carFollowModel="EIDM" color="red" probability="0.25" minGap="2" accel="1" decel="5" sigmaerror="0.9" maxSpeed="40" jmDriveRedSpeed="25" jmDriveAfterRedTime="10" emergencyDecel="5" />
        <vType id="speeder" guiShape="passenger" carFollowModel="EIDM" color="yellow" probability="0.25" tau="1.75" minGap="2.5" accel="5" decel="5" maxSpeed="60" emergencyDecel="5" />
    </vTypeDistribution>
    
    <!-- Routes -->
    <routeDistribution id="rd1">
        <route id="r_0" edges="-E1 -E0" probability="0.6"/>
        <route id="r_1" edges="E0 E1" probability="0.5"/>
        <route id="r_10" edges="-E3 E1" probability="0.6"/>
        <route id="r_11" edges="-E1 -E2" probability="0.5"/>
        <route id="r_2" edges="-E3 -E2" probability="0.4"/>
        <route id="r_3" edges="E2 E3" probability="0.4"/>
        <route id="r_4" edges="-E1 E3" probability="0.6"/>
        <route id="r_5" edges="E2 E1" probability="0.6"/>
        <route id="r_6" edges="E0 -E2" probability="0.4"/>
        <route id="r_7" edges="-E3 -E0" probability="0.5"/>
        <route id="r_8" edges="E2 -E0" probability="0.5"/>
        <route id="r_9" edges="E0 E3" probability="0.5"/>
    </routeDistribution>

    <flow id="myflow" type='vd1' route='rd1' begin="0" end="200" number="100">
    </flow>
</routes>
